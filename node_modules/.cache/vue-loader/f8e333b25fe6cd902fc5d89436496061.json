lass=\"col-lg-6 col-md-6 col-xs12\">\n        <div class=\"mt-2\">\n          <label for=\"exampleFormControlInput1\" class=\"form-label\"\n            >Correo electr√≥nico</label\n          >\n          <input\n            v-model=\"email\"\n            type=\"text\"\n            :class=\"\n              ruleEmail\n                ? 'form-control'\n                : 'form-control  is-invalid was-validated'\n            \"\n            placeholder=\"shenickk@gmail.com\"\n          />\n          <div class=\"invalid-feedback\">\n            {{ emailError }}\n          </div>\n        </div>\n      </div>\n    </div>\n    <div class=\"row\">\n      <div class=\"col-lg-3 col-md-4 col-xs12 mt-3\">\n        <button\n          type=\"button\"\n          class=\"btn btn-primary w-100\"\n          :disabled=\"!enableButton\"\n          @click=\"addUser\"\n        >\n          Agregar Usuario\n        </button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  components: {},\n  props: [],\n\n  data() {\n    return {\n      name: \"\",\n      email: \"\",\n      nameError: \"Introduce un nombre completo\",\n      emailError: \"Introduce in correo valido\",\n    }\n  },\n\n  computed: {\n    ruleName() {\n      return this.name.length < 5 ? false : true\n    },\n    ruleEmail() {\n      let emailRegex = /^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?(?:\\.[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?)*$/\n      return emailRegex.test(this.email)\n    },\n    enableButton() {\n      return this.ruleName && this.ruleEmail ? true : false\n    },\n  },\n  created() {\n    //console.log(this.$refs.nameInput.className)\n  },\n\n  mounted() {\n    console.log(this.$refs.nameInput.className)\n  },\n\n  methods: {\n    addUser() {\n      let newUser = {\n        name: this.name,\n        email: this.email,\n      }\n      this.$emit(\"addUserObj\", newUser)\n      this.name = \"\"\n      this.email = \"\"\n    },\n  },\n\n  watch: {},\n}\n</script>\n\n<style></style>\n"]}]}